<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="WechatTab">
        <attr name="pstsIndicatorColorWe" format="color" />
        <attr name="pstsUnderlineColorWe" format="color" />
        <attr name="pstsDividerColorWe" format="color" />
        <attr name="pstsIndicatorHeightWe" format="dimension" />
        <attr name="pstsUnderlineHeightWe" format="dimension" />
        <attr name="pstsDividerPaddingWe" format="dimension" />
        <attr name="pstsTabPaddingLeftRightWe" format="dimension" />
        <attr name="pstsScrollOffsetWe" format="dimension" />
        <attr name="pstsTabBackgroundWe" format="reference" />
        <attr name="pstsShouldExpandWe" format="boolean" />
        <attr name="pstsTextAllCapsWe" format="boolean" />
    </declare-styleable>
    <!--TabStrip选项卡切换属性-->
    <declare-styleable name="PagerSlidingTabStrip">
        <attr name="pstsIndicatorColor" format="color" />
        <attr name="pstsUnderlineColor" format="color" />
        <attr name="pstsDividerColor" format="color" />
        <attr name="pstsIndicatorHeight" format="dimension" />
        <attr name="pstsUnderlineHeight" format="dimension" />
        <attr name="pstsDividerPadding" format="dimension" />
        <attr name="pstsTabPaddingLeftRight" format="dimension" />
        <attr name="pstsScrollOffset" format="dimension" />
        <attr name="pstsTabBackground" format="reference" />
        <attr name="pstsShouldExpand" format="boolean" />
        <attr name="pstsTextAllCaps" format="boolean" />
    </declare-styleable>
    <declare-styleable name="MinSizeImageView">
        <attr name="minWidth" format="dimension" />
        <attr name="minHeight" format="dimension" />
        <attr name="microSize" format="dimension" />
    </declare-styleable>

    <declare-styleable name="Crossed">
        <attr name="dark" format="boolean" />
    </declare-styleable>

    <declare-styleable name="AutoComplete">
        <attr name="emailOnly" format="boolean" />
        <attr name="darkness" format="boolean" />
        <attr name="closeAutoComplete" format="boolean" />
    </declare-styleable>

    <declare-styleable name="ListItem1">
        <attr name="itemIcon" format="reference" />
        <attr name="itemTitle" format="string" />
        <attr name="itemCenter" format="boolean" />
    </declare-styleable>

    <declare-styleable name="TaskAttrItem">
        <attr name="taskIcon" format="reference" />
        <attr name="taskText1" format="string" />
        <attr name="taskText2" format="string" />
        <attr name="taskTopLine" format="boolean" />
    </declare-styleable>
    
    <declare-styleable name="IconTextView">
        <attr name="iconTextIcon" format="reference" />
        <attr name="iconTextTitle" format="string" />
    </declare-styleable>

    <declare-styleable name="IndicatorView">
        <attr name="pointCount" format="integer" />
        <attr name="pointView" format="reference" />
    </declare-styleable>

    <declare-styleable name="CountdownIndicator">
        <attr name="indicator_color" format="reference|color" />
    </declare-styleable>

    <declare-styleable name="ScanMaskView">
        <attr name="scan_width" format="dimension" />
    </declare-styleable>

    <declare-styleable name="SoundWaveView">
        <attr name="waveColor" format="color"/>
        <attr name="soundOriginOrientation">
            <enum name="left" value="1"/>
            <enum name="right" value="0"/>
        </attr>
    </declare-styleable>
    <declare-styleable name="EnterLayoutAnimSupportContainer">
        <attr name="isAdjustResize" format="boolean"/>
    </declare-styleable>
    <!-- 圆形图片 -->
    <declare-styleable name="CircleImageView">
        <attr name="border_width" format="dimension" />
        <attr name="border_color" format="color" />
    </declare-styleable>

    <declare-styleable name="LoginEditText">
        <attr name="topLine" format="boolean" />
        <attr name="hint" format="string" />
        <attr name="captcha" format="boolean" />
        <attr name="showPassword" format="boolean" />
        <attr name="loginInput" format="enum">
            <enum name="text" value="0" />
            <enum name="password" value="1" />
            <enum name="number" value="2" />
            <enum name="globalKey" value="3" />
        </attr>
        <attr name="digits" format="string" />
    </declare-styleable>

    <declare-styleable name="VoiceView">
        <attr name="InputType" format="enum" >
            <enum name="message" value="0" />
            <enum name="comment" value="1" />
        </attr>

    </declare-styleable>

    <declare-styleable name="MainInputView">
        <attr name="showEmojiOnly" format="boolean" />
    </declare-styleable>
</resources>